<main>

    <div class="row center">
        <div class="col s12 m10 offset-m1 l6 offset-l3">
            <div class="card sticky-action z-depth-4 card-panel hoverable" style=" margin : 0px; padding : 2px">

                <!-- artist search form -->
                <form>
                    <div class="card-content">
                        <div class="row center-align">
                            <h2>
                                Restaurant Search
                            </h2>
                            <span class="card-title grey-text text-darken-4 center-align">Type the name of the City to
                                search for Restaurant.</span>

                            <div class="input-field col s12">
                                <i class="material-icons prefix center"></i>
                                <input id="cityName" type='text' name='city-name' class="validate left-align" required="true"
                                    placeholder="City" />
                                <input id="stateName" type='text' name='state-name' class="validate left-align"
                                    required="true" placeholder="State" />
                                <input id="countryName" type='text' name='country-name' class="validate left-align"
                                    required="true" placeholder="Country" />
                                <label for="name" class="left-align"></label>
                            </div>

                        </div>

                        <input type="submit" class="waves-effect waves-light btn" value='Search' id="resSubmit">
                        </input>

                    </div>
                </form>

            </div>
        </div>
    </div>
</main>

